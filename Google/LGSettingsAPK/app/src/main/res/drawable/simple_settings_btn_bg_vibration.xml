<?xml version="1.0" encoding="utf-8"?>

<selector
    xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true" 
        android:drawable="@drawable/img_button_simple_setting_pressed_vibration" />
   
    <item android:state_focused="true" android:state_activated="true"
        android:drawable="@drawable/img_button_simple_setting_focused_activated_vibration" />
    
    <item android:state_focused="true"
        android:drawable="@drawable/img_button_simple_setting_normal_focused_vibration" />

    <item android:state_activated="true"
        android:drawable="@drawable/img_button_simple_setting_activated_vibration" />

    <item android:state_enabled="true"
        android:drawable="@drawable/img_button_simple_setting_normal_vibration" />
    
</selector>
