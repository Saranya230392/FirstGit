<Keyboard
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:iwnn="http://schemas.android.com/apk/res/jp.co.omronsoft.iwnnime.ml"
    iwnn:keyWidth="@dimen/popup_function_key_width"
    iwnn:horizontalGap="@dimen/popup_function_horizontal_gap"
    iwnn:verticalGap="@dimen/popup_function_vertical_gap"
    iwnn:keyHeight="@dimen/popup_function_key_height"
