            iwnn:keyIcon="@drawable/ime_keypad_input_icon_10kbd_normal" />
        <Key
            iwnn:codes="-603"
            iwnn:keyIcon="@drawable/ime_keypad_input_icon_handwriting_normal" />
        <Key
            iwnn:codes="-608"
            iwnn:keyIcon="@drawable/ime_keypad_icon_popup_cliptray_normal" />
        <Key
            iwnn:codes="-604"
            iwnn:keyIcon="@drawable/ime_keypad_input_icon_setting_normal" />
    </Row>
</Keyboard>
