<Keyboard xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:iwnn="http://schemas.android.com/apk/res/jp.co.omronsoft.iwnnime.ml"
    iwnn:keyWidth="@fraction/key_width_12key_numeric"
    iwnn:horizontalGap="@fraction/keyboard_12key_common_horizontal_gap"
    iwnn:verticalGap="@dimen/keyboard_12key_common_vertical_gap"
    iwnn:keyHeight="@dimen/keyboard_12key_common_keyheight"
    >

    <Row>
        <Key iwnn:codes="49" iwnn:keyLabel="1" iwnn:keyEdgeFlags="left" iwnn:horizontalGap="@dimen/padding_keyboard_left"/>
        <Key iwnn:codes="50" iwnn:keyLabel="2"/>
        <Key iwnn:codes="51" iwnn:keyLabel="3"/>
        <Key iwnn:codes="-214" iwnn:keyIcon="@drawable/ime_keypad_icon_jp_delete" iwnn:iconPreview="@drawable/ime_keypad_icon_jp_delete"
             iwnn:isRepeatable="true" iwnn:keyEdgeFlags="right" iwnn:horizontalGap="0%" iwnn:isSecondKey="true"/>
    </Row>

    <Row>
        <Key iwnn:codes="52" iwnn:keyLabel="4" iwnn:keyEdgeFlags="left" iwnn:horizontalGap="@dimen/padding_keyboard_left"/>
        <Key iwnn:codes="53" iwnn:keyLabel="5"/>
        <Key iwnn:codes="54" iwnn:keyLabel="6"/>
        <Key iwnn:codes="-310" iwnn:keyIcon="@drawable/ime_keypad_icon_jp_space_disabled"
             iwnn:showPreview="false" iwnn:keyEdgeFlags="right" iwnn:isSecondKey="true"/>
    </Row>

    <Row>
        <Key iwnn:codes="55" iwnn:keyLabel="7" iwnn:keyEdgeFlags="left" iwnn:horizontalGap="@dimen/padding_keyboard_left"/>
