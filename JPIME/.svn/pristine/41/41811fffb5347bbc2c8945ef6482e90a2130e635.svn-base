    <Row iwnn:rowEdgeFlags="bottom">
        <Key iwnn:codes="-310" iwnn:keyLabel=" " iwnn:keyEdgeFlags="left" iwnn:horizontalGap="@dimen/padding_keyboard_left" iwnn:showPreview="false"/>
        <Key iwnn:codes="48" iwnn:keyLabel="0" />
        <Key iwnn:codes="-310" iwnn:keyLabel=" " iwnn:showPreview="false"/>
        <Key iwnn:codes="-216" iwnn:keyIcon="@drawable/ime_keypad_icon_jp_enter"
