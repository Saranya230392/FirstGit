        <Key iwnn:codes="32"
             iwnn:keyIcon="@drawable/key_qwerty_space" iwnn:iconPreview="@drawable/key_qwerty_space_b" 
             iwnn:keyWidth="@fraction/keyboard_qwerty_space_keywidth"
             iwnn:isRepeatable="true"/>
      </Row-Key>
    </case>
    </case>
    </case>
    <!-- [case 2]SwitchIme Off | Cursor Off -->
    <case iwnn:keyboardCondition="modeHalfAlpha|switchImeOff|cursorLeftRightOff">
    <case iwnn:keyboardCondition="modeHalfNum|switchImeOff|cursorLeftRightOff">
    <case iwnn:keyboardCondition="modeHalfKatakana|switchImeOff|cursorLeftRightOff">
      <Row-Key>
        <Key iwnn:codes="32"
             iwnn:keyIcon="@drawable/key_qwerty_space" iwnn:iconPreview="@drawable/key_qwerty_space_b" 
             iwnn:keyWidth="@fraction/keyboard_qwerty_space_cursordisable_keywidth"
             iwnn:isRepeatable="true"/>
      </Row-Key>
    </case>
    </case>
    </case>
    <!-- [case 3]SwitchIme On | Cursor On -->
    <case iwnn:keyboardCondition="modeHalfAlpha|switchImeOn|cursorLeftRightOn">
    <case iwnn:keyboardCondition="modeHalfNum|switchImeOn|cursorLeftRightOn">
    <case iwnn:keyboardCondition="modeHalfKatakana|switchImeOn|cursorLeftRightOn">
      <Row-Key>
        <Key iwnn:codes="32"
             iwnn:keyIcon="@drawable/key_qwerty_space" iwnn:iconPreview="@drawable/key_qwerty_space_b" 
             iwnn:keyWidth="@fraction/keyboard_qwerty_space_switchime_keywidth"
             iwnn:isRepeatable="true"/>
      </Row-Key>
    </case>
    </case>
    </case>
    <!-- [case 4]SwitchIme On | Cursor Off -->
    <case iwnn:keyboardCondition="modeHalfAlpha|switchImeOn|cursorLeftRightOff">
    <case iwnn:keyboardCondition="modeHalfNum|switchImeOn|cursorLeftRightOff">
    <case iwnn:keyboardCondition="modeHalfKatakana|switchImeOn|cursorLeftRightOff">
      <Row-Key>
        <Key iwnn:codes="32"
             iwnn:keyIcon="@drawable/key_qwerty_space" iwnn:iconPreview="@drawable/key_qwerty_space_b" 
             iwnn:keyWidth="@fraction/keyboard_qwerty_space_switchime_cursordisable_keywidth"
             iwnn:isRepeatable="true"/>
      </Row-Key>
    </case>
    </case>
    </case>

  </switch>
</Keyboard>
